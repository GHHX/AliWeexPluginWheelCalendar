<?xml version="1.0" encoding="utf-8"?>
<!-- only one that in use -->
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:color="@color/calendar_text_inactive" app:tsquare_state_current_month="false"/>
    <item android:color="@color/calendar_text_unselectable" app:tsquare_state_selectable="false"/>
    <item android:color="@color/white" app:tsquare_state_range_first="true" />
    <item android:color="@color/white" app:tsquare_state_range_last="true" />
    <!--<item android:color="@color/calendar_normal" app:tsquare_state_range_middle="true"/>-->
    <item android:color="@color/calendar_today" app:tsquare_state_future="true"/>
    <item android:color="@color/calendar_selected" android:state_selected="true"/>
    <item android:color="@color/calendar_selected" android:state_pressed="true"/>
    <item android:color="@color/calendar_selected" app:tsquare_state_highlighted="true"/>
    <item android:color="@color/calendar_today" app:tsquare_state_today="true"/>
    <item android:color="@color/calendar_holiday" app:tsquare_state_holiday="true"/>
    <item android:color="@color/calendar_normal"/>
</selector>
