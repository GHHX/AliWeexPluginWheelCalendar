<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:drawable="@drawable/shape_left_round_rect_blue" app:tsquare_state_range_first="true"/>
    <item android:drawable="@drawable/shape_right_round_rect_blue" app:tsquare_state_range_last="true"/>

    <item app:tsquare_state_range_middle="true">
        <color android:color="@color/calendar_range_middle"/>
    </item>
    <item android:drawable="@drawable/round_rect_blue" android:state_selected="true">
    </item>
    <item android:drawable="@drawable/round_rect_blue" android:state_pressed="true">
    </item>
    <item app:tsquare_state_current_month="false">
        <color android:color="#cccccc"/>
    </item>
    <item app:tsquare_state_highlighted="true">
        <color android:color="@color/calendar_selected"/>
    </item>
    <item app:tsquare_state_today="true">
        <color android:color="@color/white"/>
    </item>
    <item>
        <color android:color="@color/white"/>
    </item>
</selector>
